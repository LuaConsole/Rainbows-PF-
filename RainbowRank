local fakerank = game:GetService("Players").purplevalenYT.PlayerGui.MenuScreenGui.Pages.PageMainMenu.DisplayPlayerCard.ContainerLeft.DisplayRank.TextRight
fakerank.Text = "1000" -- change this to whatever rank u want, or delete it completely
local top = 100
while true do
wait()
for i = 1,top,1 do
fakerank.TextColor3 = Color3.new(i/top,0/top,0/top)
wait()
end
for i = 1,top,1 do
fakerank.TextColor3 = Color3.new(top/top,i/top,0/top)
wait()
end
for i = 1,top,1 do
fakerank.TextColor3 = Color3.new(top/top,top/top,i/top)
wait()
end
for i = top,1,-1 do
fakerank.TextColor3 = Color3.new(i/top,top/top,top/top)
wait()
end
for i = top,1,-1 do
fakerank.TextColor3 = Color3.new(0/top,i/top,top/top)
wait()
end
for i = top,1,-1 do
fakerank.TextColor3 = Color3.new(0/top,0/top,i/top)
wait()
end
end
